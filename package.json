{
  "name": "clean-the-mess",
  "version": "1.0.0",
  "description": "Hundreds of opened tabs? Dozens of useless duplicate tabs with opened emails, google searches, your favourite social networks and articles you wanted to read a month ago? Let's clean the mess!",
  "main": "src/popup/popup.js",
  "scripts": {
    "prebuild": "gulp prebuild --b $BROWSER",
    "build": "NODE_ENV=$BROWSER parcel build dist/.$BROWSER-temp/manifest.json --no-content-hash --no-cache --no-source-maps --dist-dir dist/$BROWSER",
    "postbuild": "rm -rf dist/.$BROWSER-temp",
    "build:chrome": "BROWSER=chrome npm run build",
    "build:firefox": "BROWSER=firefox npm run build",
    "build:firefox:src": "dir-archiver --src . --dest ./build/firefox/src_clean_the_mess.zip --exclude build dist .vscode .parcel-cache node_modules src/_dev .git **/.DS_Store",
    "webext:build:firefox": "web-ext build -a ./build/firefox -s ./dist/firefox/ && npm run build:firefox:src",
    "webext:build:chrome": "web-ext build -a ./build/chrome -s ./dist/chrome/",
    "test": "jest",
    "start": "npm run chrome && npm run firefox",
    "firefox": "web-ext run -s ./dist/firefox --bc",
    "chrome": "web-ext run -t chromium -s ./dist/chrome --bc",
    "dev:chrome": "gulp dev --b chrome & NODE_ENV=chrome parcel src/manifest.json --dist-dir dist/chrome",
    "dev:firefox": "gulp dev --b firefox & NODE_ENV=firefox parcel src/manifest.json --dist-dir dist/firefox",
    "lint": "web-ext lint -s",
    "dev:ui": "webpack serve",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomas-hartman/clean-the-mess.git"
  },
  "keywords": [
    "addon",
    "mozilla",
    "tabs"
  ],
  "author": "Tomas Hartman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomas-hartman/clean-the-mess/issues"
  },
  "homepage": "https://github.com/tomas-hartman/clean-the-mess#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@parcel/config-webextension": "^2.8.3",
    "@parcel/transformer-js": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@parcel/transformer-svg-react": "^2.8.3",
    "@parcel/transformer-webextension": "^2.8.3",
    "@storybook/addon-essentials": "^7.0.0-rc.3",
    "@storybook/addon-interactions": "^7.0.0-rc.3",
    "@storybook/addon-links": "^7.0.0-rc.3",
    "@storybook/blocks": "^7.0.0-rc.3",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.0.0-rc.3",
    "@storybook/react-webpack5": "^7.0.0-rc.3",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@svgr/webpack": "^6.5.1",
    "@types/react-dom": "^17.0.2",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "babel-jest": "^27.5.1",
    "css-loader": "^6.7.3",
    "dir-archiver": "^1.2.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.6.11",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "jest": "^27.5.1",
    "node-sass": "^7.0.1",
    "parcel": "^2.8.3",
    "prettier": "2.8.4",
    "prop-types": "^15.8.1",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.0",
    "storybook": "^7.0.0-rc.3",
    "style-loader": "^3.3.2",
    "typescript": "^4.8.4",
    "web-ext": "^6.1.0",
    "yargs": "^7.1.2"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "gulp-merge-json": "^2.1.1",
    "gulp-rename": "^2.0.0",
    "gulp-template-html": "^0.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webextension-polyfill": "^0.10.0"
  },
  "targets": {
    "main": false
  },
  "webExt": {
    "sourceDir": "dist/test"
  }
}
